def pomnoz_przez_dwa_for(lista_liczb):
    wynik = []
    for liczba in lista_liczb:
        wynik.append(liczba * 2)
    return wynik


print(pomnoz_przez_dwa_for([1, 2, 3, 4, 5]))

#
def pomnoz_przez_dwa_skladana(lista_liczb):
    return [liczba * 2 for liczba in lista_liczb]

# 
print(pomnoz_przez_dwa_skladana([1, 2, 3, 4, 5]))